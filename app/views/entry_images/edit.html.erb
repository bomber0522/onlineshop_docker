<% provide(:title, 'ブログ記事の画像編集') %>
<div class="entry-images-new">ブログ記事の画像編集</div>
<h2><%= @entry.title %></h2>

<% url = entry_image_path(@entry, @image) %>
<%= form_for @image, as: "image", url: url do |form| %>
  <%= render "form", form: form %>
  <div><%= form.submit '更新', class: 'btn btn-primary' %></div>
<% end %>
