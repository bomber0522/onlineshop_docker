<% provide(:title, 'ブログ記事への画像追加') %>
<div class="entry-images-new">ブログ記事への画像追加</div>
<h2><%= @entry.title %></h2>

<% url = entry_images_path(@entry, @image) %>
<%= form_for @image, as: "image", url: url do |form| %>
  <%= render "form", form: form %>
  <div><%= form.submit '登録', class: 'btn btn-primary' %></div>
<% end %>
